option_settings:
  "aws:elasticbeanstalk:application:environment":
    DJANGO_SETTINGS_MODULE: "config.settings.prod"
    "PYTHONPATH": "/opt/python/current/app/sirius/sirius:/opt/python/current/app/sirius:$PYTHONPATH"
  "aws:elasticbeanstalk:container:python":
    WSGIPath: sirius/config/wsgi.py
    NumProcesses: 3
    NumThreads: 20
  "aws:elasticbeanstalk:container:python:staticfiles":
    "/static/": "www/static/"
